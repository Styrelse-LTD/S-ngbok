{
    "latex-workshop.latex.tools": [
      {
        "name": "Python Script to Sort Table of Content",
        "command": "python",
        "args": [
          "%DIR%/AlphaBetatoc.py"
        ],
        "env": {}
      },
      {
        "name": "pdflatex",
        "command": "pdflatex",
        "args": [
          "-synctex=1",
          "-interaction=nonstopmode",
          "-file-line-error",
          "%DOC%"
        ]
      }
    ],
    "latex-workshop.latex.recipes": [
      {
        "name": "pdfLaTeX+Python+pdfLaTeX",
        "tools": [
          "pdflatex",
          "Python Script to Sort Table of Content",
          "pdflatex"
        ]
      }
    ],
    "latex-workshop.latex.autoBuild.run": "onSave"
  }